{
  "name": "vue3-js-ts-webpack5-multi-env",
  "version": "1.0.0",
  "description": "a demo project by vue3, pure js writing method, supports setup, supports ts parsing, uses webpack5 for packaging, supports multiple environments and multiple variables.",
  "author": "mmpanzl@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config build/webpack.dev.js",
    "dev-domesticApp": "cross-env NODE_ENV=development webpack-dev-server --no-hot --config build/webpack.dev.js",
    "build-domesticApp-test": "webpack --config build/webpack.test.js",
    "build-domesticApp-uat": "webpack --config build/webpack.uat.js",
    "build-domesticApp-prod": "webpack --config build/webpack.prod.js",
    "dev-globalApp": "cross-env NODE_ENV=development webpack-dev-server --no-hot --config build/webpack.dev.js",
    "build-globalApp-test": "webpack --config build/webpack.test.js",
    "build-globalApp-uat": "webpack --config build/webpack.uat.js",
    "build-globalApp-prod": "webpack --config build/webpack.prod.js",
    "distanalyzer": "webpack --env.analyzer=true --config build/webpack.prod.js",
    "lint-fix": "eslint --fix --ext .js --ext .vue public/pages",
    "format:prettier": "prettier --write \"public/**/*.{js,ts,json,tsx,vue,md}\""
  },
  "dependencies": {
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime": "^7.24.4",
    "@babel/runtime-corejs3": "^7.24.4",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@vue/compiler-sfc": "^3.4.27",
    "@vueuse/core": "^10.9.0",
    "autoprefix": "^1.0.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "js-md5": "^0.8.3",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "terser": "^5.30.3",
    "terser-webpack-plugin": "^5.3.10",
    "thread-loader": "^4.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-define-options": "^1.4.3",
    "unplugin-vue-setup-extend-plus": "^1.0.1",
    "vue-loader": "^17.4.2",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.10.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
